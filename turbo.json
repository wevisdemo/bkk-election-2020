{
  "pipeline": {
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false
    },
    "dev:landing": {
      "dependsOn": [
        "landing#dev",
        "ui#dev",
        "moderator#dev",
        "wordpress-api#dev"
      ],
      "cache": false
    },
    "dev:candidate": {
      "dependsOn": [
        "candidate#dev",
        "ui#dev",
        "moderator#dev",
        "wordpress-api#dev"
      ],
      "cache": false
    },
    "dev:socialtrend": {
      "dependsOn": ["socialtrend#dev", "ui#dev", "moderator#dev"],
      "cache": false
    },
    "dev:map": {
      "dependsOn": ["map#dev", "ui#dev", "moderator#dev"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "$BUILD_ENV"],
      "outputs": ["dist/**", "build/**", "out/**"]
    },
    "lint": {
      "outputs": []
    }
  }
}
